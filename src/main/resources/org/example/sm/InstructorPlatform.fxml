<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.InstructorPlatform">
   <children>
      <Pane layoutX="80.0" layoutY="147.0" prefHeight="542.0" prefWidth="890.0" style="-fx-background-color: white;">
         <children>
            <ButtonBar prefHeight="40.0" prefWidth="844.0">
               <buttons>
                  <Button fx:id="Course" mnemonicParsing="false" onAction="#CourseLecinfo" text="Course" textAlignment="CENTER" />
                  <Button mnemonicParsing="false" onAction="#StudentList" text="Student" />
                  <Button mnemonicParsing="false" text="Annouce" />
                  <Button mnemonicParsing="false" text="Exam Date" />
                  <Button mnemonicParsing="false" text="Grading" />
                  <Button mnemonicParsing="false" prefHeight="26.0" prefWidth="142.0" text="Personal Information" />
               </buttons>
            </ButtonBar>
         </children>
      </Pane>
      <Label fx:id="nameLabel" layoutX="80.0" layoutY="100.0" text="Label">
         <font>
            <Font name="Cambria" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
